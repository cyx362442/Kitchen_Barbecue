<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_low"
    tools:context="com.duowei.kitchen_barbecue.app.MainActivity">
    <FrameLayout
        android:id="@+id/frame_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></FrameLayout>
    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_below="@+id/frame_top"
        android:background="@drawable/shape_border"></View>
    <FrameLayout
        android:id="@+id/frame_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/view"></FrameLayout>
    <FrameLayout
        android:id="@+id/frame_out"
        android:layout_width="500dp"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:layout_below="@+id/view"
        android:layout_alignParentRight="true"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"></FrameLayout>
</RelativeLayout>
